	<queryString>
		<![CDATA[SELECT
 e.nombre as edificio,
 p.nombre as piso,
 o.nombre as oficina,
 es.nombre as nombre_establecimiento
FROM
 edificios e,
 pisos p,
 oficinas o,
 establecimientos es
WHERE
 e.id=p.edificio_id AND
 p.id=o.piso_id AND
 e.establecimiento_id = $P{establecimientoID} AND
 $X{IN, e."id", edificioID} AND
 $X{IN, p."id", pisoID}
ORDER BY
 e.nombre,p.nombre,o.nombre]]>
	</queryString>
